<?xml version="1.0"?>
<Container version="2">
  <Name>Discord-Bot</Name>
  <Repository>groveld/discord-bot</Repository>
  <Registry>https://hub.docker.com/r/groveld/discord-bot</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Privileged>false</Privileged>
  <Support>https://discord.gg/2dV4xRN</Support>
  <Project>https://github.com/groveld/discordjs-bot</Project>
  <Overview>Discord-Bot is a general-purpose Discord bot build with the Discord.js library.</Overview>
  <Category/>
  <WebUI>http://[IP]:[PORT:5000]/</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/groveld/discord-bot/master/unraid/discord-bot.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/groveld/discord-bot/master/unraid/discord-bot.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <DateInstalled/>
  <DonateText/>
  <DonateLink/>
  <DonateImg/>
  <MinVer/>
  <Description>Discord-Bot is a general-purpose Discord bot build with the Discord.js library.</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5000</HostPort>
        <ContainerPort>5000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/discord-bot</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>123abc</Value>
      <Name>bot_token</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>123abc</Value>
      <Name>bot_secret</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>123abc</Value>
      <Name>bot_owner</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>123abc</Value>
      <Name>bot_salt</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>http://localhost:5000/callback</Value>
      <Name>bot_callback</Name>
      <Mode/>
    </Variable>
  </Environment>
</Container>
