<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Discord-Bot</Name>
  <Description>A Discord Bot</Description>
  <Registry>https://hub.docker.com/r/groveld/discord-bot/</Registry>
  <Repository>groveld/discord-bot</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5000</HostPort>
        <ContainerPort>5000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/discord-bot</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>05616fd2</Version>
  <WebUI>http://[IP]:[PORT:5000]/</WebUI>
  <Banner>https://raw.githubusercontent.com/groveld/discord-bot/master/unraid/discord-bot.png</Banner>
  <Icon>https://raw.githubusercontent.com/groveld/discord-bot/master/unraid/discord-bot.png</Icon>
  <ExtraParams></ExtraParams>
</Container>
